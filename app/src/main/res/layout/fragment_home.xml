                    app:cardElevation="5dp"
                    tools:targetApi="LOLLIPOP">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="8dp"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/plumbingServiceIV"
                            android:layout_width="65dp"
                            android:layout_height="65dp"
                            android:src="@drawable/plumbing"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="Electric &amp; Plumbing"
                            android:textAppearance="@style/monospaceSmallText"
                            app:layout_constraintEnd_toEndOf="@id/plumbingServiceIV"
                            app:layout_constraintStart_toStartOf="@id/plumbingServiceIV"
                            app:layout_constraintTop_toBottomOf="@+id/plumbingServiceIV" />

                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>


