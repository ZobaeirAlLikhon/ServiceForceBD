                            app:layout_constraintStart_toStartOf="@id/applianceIV"
                            app:layout_constraintTop_toBottomOf="@+id/applianceIV" />

                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
